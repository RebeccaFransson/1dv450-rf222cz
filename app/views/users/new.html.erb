
<div class="page-header">
  <h1>Register new user</h1>
  <%=	 link_to 'Back to login', root_path %>
</div>

<%=	  simple_form_for @user do |f| %>
    <%=	 f.input :name %>
    <%=	 f.input :email %>
    <%=	 f.input :password %>
    <%=	 f.input :password_confirmation %>
    <%=	 f.button :submit %>
<% end %>